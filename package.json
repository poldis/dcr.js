	},
	"devDependencies": {
		"@eslint/js": "^9.2.0",
		"@types/node": "^17.0.41",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.2.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"prettier": "3.2.5",
		"typescript": "^5.4.5",
		"typescript-eslint": "^7.8.0"
	},
	"publishConfig": {},
	"repository": "git://github.com/poldis/dcr.js.git",
	"bugs": {
		"url": "https://github.com/poldis/dcr.js/issues"
