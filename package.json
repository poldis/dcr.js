{
	"name": "dcr.js",
	"version": "1.0.23",
	"description": "A Wrapper for the DCR API, DB & Cache",
	"main": "dist/index.js",
	"author": "Leopold Scharlach",
	"license": "MIT",
	"scripts": {
		"format:check": "prettier ./src --check",
		"format:write": "prettier ./src --write",
		"lint:check": "eslint ./src/**",
		"lint:fix": "eslint --fix",
